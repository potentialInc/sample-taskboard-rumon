{
    "name": "nestjs-starter-kit",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "prepare": "husky",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "concurrently \"npm:start:dev:server\" \"npm:lint:watch\" --names \"SERVER,LINT\" --prefix-colors \"cyan,yellow\"",
        "start:dev:server": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
        "lint:watch": "nodemon --watch src --watch test --ext ts --exec \"npm run lint:check\"",
        "type-check": "tsc --noEmit",
        "type-check:watch": "tsc --noEmit --watch",
        "test": "jest",
        "test:ci": "jest --ci --coverage --maxWorkers=2",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d src/config/db.config.ts",
        "migration:create": "cross-env ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli migration:create ./src/database/migrations/%npm_config_name%",
        "migration:generate": "cross-env npm run typeorm -- migration:generate ./src/database/migrations/%npm_config_name%",
        "migration:run": "npm run typeorm -- migration:run",
        "migration:revert": "npm run typeorm -- migration:revert",
        "migration:show": "npm run typeorm -- migration:show",
        "migration:dev-run": "npm run build && npm run typeorm -- migration:run",
        "seed:run": "ts-node -r tsconfig-paths/register ./src/database/seeders/seed.helper.ts",
        "seed:reset-users": "ts-node -r tsconfig-paths/register ./src/database/seeders/reset-users.ts",
        "docker:dev": "docker-compose --profile dev up",
        "docker:dev:build": "docker-compose --profile dev up --build",
        "docker:dev:down": "docker-compose down",
        "docker:prod": "docker-compose --profile prod up -d",
        "docker:prod:build": "docker-compose --profile prod up --build -d",
        "docker:prod:down": "docker-compose --profile prod down",
        "docker:logs": "docker-compose logs -f",
        "docker:clean": "docker-compose down -v"
    },
    "dependencies": {
        "@arendajaelu/nestjs-passport-apple": "^2.0.5",
        "@aws-sdk/client-s3": "^3.775.0",
        "@aws-sdk/lib-storage": "^3.775.0",
        "@aws-sdk/s3-request-presigner": "^3.990.0",
        "@nestjs/common": "^11.0.1",
        "@nestjs/config": "^4.0.2",
        "@nestjs/core": "^11.0.1",
        "@nestjs/jwt": "^11.0.0",
        "@nestjs/microservices": "^11.1.6",
        "@nestjs/passport": "^11.0.5",
        "@nestjs/platform-express": "^11.0.1",
        "@nestjs/platform-socket.io": "^11.1.13",
        "@nestjs/schedule": "^6.1.1",
        "@nestjs/serve-static": "^5.0.3",
        "@nestjs/swagger": "^11.1.0",
        "@nestjs/throttler": "^6.4.0",
        "@nestjs/typeorm": "^11.0.0",
        "@nestjs/websockets": "^11.1.13",
        "@sendgrid/mail": "^8.1.6",
        "@types/passport-google-oauth20": "^2.0.17",
        "@types/passport-jwt": "^4.0.1",
        "axios": "^1.13.1",
        "basic-auth": "^2.0.1",
        "bcrypt": "^5.1.1",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "cookie-parser": "^1.4.7",
        "dotenv": "^17.2.2",
        "express": "^5.1.0",
        "express-basic-auth": "^1.2.1",
        "firebase-admin": "^13.5.0",
        "google-auth-library": "^10.4.0",
        "helmet": "^8.1.0",
        "i18next": "^25.3.4",
        "ioredis": "^5.7.0",
        "jose": "^6.0.12",
        "jsonwebtoken": "^9.0.2",
        "multer": "^2.0.1",
        "nest-winston": "^1.10.2",
        "nestjs-i18n": "^10.5.1",
        "nodemailer": "^7.0.9",
        "openai": "^5.9.0",
        "passport": "^0.7.0",
        "passport-google-oauth20": "^2.0.0",
        "passport-jwt": "^4.0.1",
        "pg": "^8.14.1",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.1",
        "socket.io": "^4.8.3",
        "typeorm": "^0.3.27",
        "typeorm-naming-strategies": "^4.1.0",
        "uuid": "^13.0.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^20.1.0",
        "@commitlint/config-conventional": "^20.0.0",
        "@eslint/eslintrc": "^3.2.0",
        "@eslint/js": "^9.18.0",
        "@nestjs/cli": "^11.0.0",
        "@nestjs/schematics": "^11.0.0",
        "@nestjs/testing": "^11.0.1",
        "@types/bcrypt": "^6.0.0",
        "@types/cookie-parser": "^1.4.10",
        "@types/dotenv": "^6.1.1",
        "@types/express": "^5.0.0",
        "@types/jest": "^30.0.0",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/multer": "^1.4.12",
        "@types/node": "^22.10.7",
        "@types/nodemailer": "^7.0.3",
        "@types/supertest": "^6.0.2",
        "@typescript-eslint/eslint-plugin": "^8.25.0",
        "@typescript-eslint/parser": "^8.25.0",
        "concurrently": "^9.2.1",
        "cross-env": "^10.0.0",
        "eslint": "^9.18.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-prettier": "^5.2.2",
        "esw": "^0.15.0",
        "globals": "^16.0.0",
        "husky": "^9.1.7",
        "jest": "^30.0.0",
        "lint-staged": "^16.2.6",
        "nodemon": "^3.1.10",
        "prettier": "^3.4.2",
        "source-map-support": "^0.5.21",
        "supertest": "^7.0.0",
        "ts-jest": "^29.2.5",
        "ts-loader": "^9.5.2",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.7.3",
        "typescript-eslint": "^8.20.0"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node",
        "moduleNameMapper": {
            "^src/(.*)$": "<rootDir>/$1"
        },
        "modulePaths": [
            "<rootDir>"
        ]
    }
}
